eOf(++currVal))&#10;metadata.put(&quot;COUNT&quot;,8)&#10;metadata.put(&quot;STATE&quot;,0)&#10;&#10;if (currVal == 6) {&#10;&#9;metadata.put(&quot;IDX&quot;, &quot;1&quot;)&#10;}" VISIBLE="false"/></PARAMETERS><IMAGE_MEASUREMENTS/><OBJECT_MEASUREMENTS/><METADATA/></MODULE><MODULE CLASSNAME="io.github.mianalysis.mia.module.script.RunScript" DISABLEABLE="false" ENABLED="true" ID="166181099" NAME="Run script" NICKNAME="Run script" NOTES="" SHOW_BASIC_TITLE="true" SHOW_OUTPUT="false" VERSION="1.0.2"><PARAMETERS><PARAMETER NAME="Script mode" NICKNAME="Script mode" SHOW_TEXT="false" VALUE="Script text" VISIBLE="false"/><PARAMETER NAME="Script language" NICKNAME="Script language" SHOW_TEXT="false" VALUE="Groovy" VISIBLE="false"/><PARAMETER NAME="Script text" NICKNAME="Script text" VALUE="// The following two parameters will provide references to the workspace and current module.&#10;#@ io.github.mianalysis.mia.object.Workspace workspace&#10;#@ io.github.mianalysis.mia.module.Module thisModule&#10;&#10;import java.util.Arrays&#10;import java.util.HashMap&#10;import java.util.List&#10;&#10;import org.apache.commons.text.similarity.LevenshteinDistance&#10;&#10;dictionary = new HashMap&lt;String,String[]&gt;()&#10;dictionary.put(&quot;1&quot;,new String[]{&quot;shell&quot;,&quot;a shell&quot;})&#10;dictionary.put(&quot;2&quot;,new String[]{&quot;finger&quot;,&quot;fingertip&quot;,&quot;fingerprint&quot;,&quot;thumb&quot;,&quot;thumbprint&quot;, &quot;a finger&quot;,&quot;a fingerprint&quot;})&#10;dictionary.put(&quot;3&quot;,new String[]{&quot;feather&quot;, &quot;a feather&quot;})&#10;dictionary.put(&quot;4&quot;,new String[]{&quot;popcorn&quot;, &quot;popcorns&quot;, &quot;popping corn&quot;, &quot;popcorn kernels&quot;})&#10;dictionary.put(&quot;5&quot;,new String[]{&quot;leaf&quot;, &quot;green leaf&quot;, &quot;a leaf&quot;, &quot;foliage&quot;})&#10;&#10;maxDistance = 2&#10;&#10;metadata = workspace.getMetadata()&#10;&#10;// Only do something when a guess has been made&#10;&#10;if (metadata.get(&quot;GUESS&quot;).equals(&quot;&quot;)) {&#10;&#9;metadata.put(&quot;RESULT&quot;,&quot;&quot;)&#10;&#9;return&#10;}&#10;&#10;levenshtein = new LevenshteinDistance()&#10;bestMatch = null&#10;bestDistance = Integer.MAX_VALUE&#10;&#10;for (word:dictionary.get(metadata.get(&quot;IDX&quot;))) {&#10;    distance = levenshtein.apply(metadata.get(&quot;GUESS&quot;), word)&#10;    if (distance &lt; bestDistance) {&#10;        bestMatch = word&#10;        bestDistance = distance&#10;    }&#10;}&#10;&#10;currVal = Integer.parseInt((String) metadata.get(&quot;COUNT&quot;));&#10;System.out.println(currVal)&#10;if (bestDistance &lt;= maxDistance) {&#10;    metadata.put(&quot;COUNT&quot;,&quot;1&quot;)&#10;&#9;metadata.put(&quot;STATE&quot;,&quot;1&quot;)&#10;} else {&#10;    metadata.put(&quot;RESULT&quot;,&quot;That's a great guess, but try again&quot;)&#10;&#9;metadata.put(&quot;COUNT&quot;, String.valueOf(--currVal))&#10;}&#10;&#10;" VISIBLE="false"/><PARAMETER NAME="Script file" NICKNAME="Script file" SHOW_TEXT="false" VALUE="" VISIBLE="false"/><PARAMETER NAME="Add parameter" NICKNAME="Add parameter" VALUE="" VISIBLE="false"><COLLECTION/></PARAMETER><PARAMETER NAME="Add output" NICKNAME="Add output" VALUE="" VISIBLE="false"><COLLECTION><PARAMS><PARAM NAME="Output type" NICKNAME="Output type" SHOW_TEXT="false" VALUE="Metadata" VISIBLE="false"/><PARAM NAME="Output image" NICKNAME="Output image" VALUE="" VISIBLE="false"/><PARAM NAME="Output objects" NICKNAME="Output objects" VALUE="" VISIBLE="false"/><PARAM NAME="Associated image" NICKNAME="Associated image" SHOW_TEXT="false" VALUE="" VISIBLE="false"/><PARAM NAME="Metadata name" NICKNAME="Metadata name" VALUE="RESULT" VISIBLE="false"/><PARAM NAME="Associated objects" NICKNAME="Associated objects" SHOW_TEXT="false" VALUE="" VISIBLE="false"/><PARAM NAME="Measurement name" NICKNAME="Measurement name" VALUE="" VISIBLE="false"/><PARAM NAME="Object metadata name" NICKNAME="Object metadata name" VALUE="" VIS